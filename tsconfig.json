{
  "compilerOptions": {
    "target": "esnext",
    "lib": [
      "esnext"
    ],

    "module": "esnext",
    "moduleResolution": "node",
    "paths": {},
    "types": [
      "discord.js",
      "knex"
    ],
    "outDir": "./build",
    "removeComments": true,
    "preserveConstEnums": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    "strict": true,
    "alwaysStrict": true,
    "skipLibCheck": true
  },

  "exclude": ["./node_modules/**/*"]
}
